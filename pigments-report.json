      ".source.css",
      ".source.css.less",
      ".source.sass",
      ".source.css.scss",
      ".source.stylus"
    ],
    "maxDecorationsInGutter": 100,
    "sassShadeAndTintImplementation": "compass",
    "groupPaletteColors": "none",
    "delayBeforeScan": 500,
    "ignoreVcsIgnoredPaths": true,
    "notifyReloads": true
  },
  "project": {
    "config": {},
    "paths": [
      "C:\\Users\\tuoma\\Documents\\Projects\\atom\\rainbow\\**"
    ],
